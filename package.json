{
  "name": "meu-inss-web-app",
  "version": "1.0.0",
  "description": "Sistema web para escritório INSS e Justiça com backend local",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "inss",
    "justiça",
    "escritório",
    "web-app"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "sqlite3": "^5.1.7"
  }
}
